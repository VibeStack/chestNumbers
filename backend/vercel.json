{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://chest-numbers-fa.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": [
        "OPTIONS"
      ],
      "dest": "./index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./index.js"
    }
  ]
}